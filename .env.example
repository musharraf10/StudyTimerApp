# Firebase Configuration
EXPO_PUBLIC_FIREBASE_API_KEY=your_firebase_api_key_here
EXPO_PUBLIC_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
EXPO_PUBLIC_FIREBASE_PROJECT_ID=your_project_id
EXPO_PUBLIC_FIREBASE_STORAGE_BUCKET=your_project.appspot.com
EXPO_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=123456789012
EXPO_PUBLIC_FIREBASE_APP_ID=1:123456789012:web:abcdefghijklmnop
EXPO_PUBLIC_FIREBASE_MEASUREMENT_ID=G-XXXXXXXXXX

# FCM Configuration (for web notifications)
EXPO_PUBLIC_FCM_VAPID_KEY=your_vapid_key_here

# App Configuration
EXPO_PUBLIC_APP_NAME=FocusVault
EXPO_PUBLIC_APP_VERSION=1.0.0

# Development Configuration
EXPO_PUBLIC_DEV_MODE=true
EXPO_PUBLIC_ENABLE_ANALYTICS=true
EXPO_PUBLIC_ENABLE_NOTIFICATIONS=true

# API Configuration (if using external APIs)
EXPO_PUBLIC_API_BASE_URL=https://api.focusvault.com
EXPO_PUBLIC_API_VERSION=v1